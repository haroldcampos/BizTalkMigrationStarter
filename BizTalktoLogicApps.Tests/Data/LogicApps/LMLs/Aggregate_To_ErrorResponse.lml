        $@ErrorDescription: /ns0:Root/InputMessagePart_2/LastPaymentResponse/ResponseStatus/Error/@ErrorDescription
        $@ErrorNumber: /ns0:Root/InputMessagePart_2/LastPaymentResponse/ResponseStatus/Error/@ErrorNumber
        $@ErrorSource: /ns0:Root/InputMessagePart_2/LastPaymentResponse/ResponseStatus/Error/@ErrorSource
      PendingTransactionsError:
        $@ErrorDescription: /ns0:Root/InputMessagePart_3/PendingTransactionsResponse/ResponseStatus/Error/@ErrorDescription
        $@ErrorNumber: /ns0:Root/InputMessagePart_3/PendingTransactionsResponse/ResponseStatus/Error/@ErrorNumber
